{
  "name": "vue2-express-sample",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch2": "docker container run -it --rm -u $(id -u $USER) -v \"$PWD\":/src \"$PACK\" webpack --watch --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "build": "docker container run -it --rm -u $(id -u $USER) -v \"$PWD\":/src \"$PACK\" webpack --config webpack.config.js",
    "release": "hg update release -C",
    "tsc2": "docker container run -it --rm  -u $(id -u $USER) -v \"$PWD\":/src \"$TSC\" tsc --watch",
    "tsc": "tsc --watch",
    "run": "node bin/www.js",
    "runserver": "sudo forever start -l ./log.txt -a bin/www.js",
    "stopserver": "sudo forever stop bin/www.js",
    "test": "jasmine"
  },
  "dependencies": {
    "@types/lodash": "^4.14.180",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "debug": "^2.6.9",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "jasmine": "^3.7.0",
    "less-middleware": "^3.1.0",
    "lodash": "^4.17.21",
    "pug": "^3.0.2",
    "underscore": "^1.13.1",
    "url-loader": "^4.1.1",
    "vuex": "^3.6.2"
  },
  "description": "My webpack project",
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/node": "^15.3.0",
    "css-loader": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "pug-plain-loader": "^1.1.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-loader": "^15.9.6",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
}
